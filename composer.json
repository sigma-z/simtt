{
    "name": "sigma-z/simtt",
    "description": "Interactive Simple Command Line Time Tracker in PHP",
    "type": "project",
    "keywords": [
        "time tracker",
        "cli timer",
        "work timer"
    ],
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3.7",
        "sensiolabs-de/deptrac-shim": "^1.0",
        "mikey179/vfsstream": "^1.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "sigma-z",
            "email": "sigma_z@sigma-scripts.de"
        }
    ],
    "support": {
        "issues": "https://github.com/sigma-z/simtt/issues"
    },
    "bin": [
        "simtt"
    ],
    "require": {
        "php": ">=7.2",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/config": "^6.0",
        "symfony/yaml": "^6.0",
        "ext-json": "*"
    },
    "autoload" : {
        "psr-4" : {
            "Simtt\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Test\\" : "tests"
        }
    }
}
