{
    "name": "sigma-z/simtt",
    "description": "Interactive Simple Command Line Time Tracker in PHP",
    "type": "project",
    "keywords": [
        "time tracker",
        "cli timer",
        "work timer"
    ],
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.5",
        "adlawson/vfs": "*",
        "sensiolabs-de/deptrac-shim": "^0.6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "sigma-z",
            "email": "sigma_z@sigma-scripts.de"
        }
    ],
    "support": {
        "issues": "https://github.com/sigma-z/simtt/issues"
    },
    "bin": [
        "simtt"
    ],
    "require": {
        "php": "^7.2",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/config": "^4.4",
        "symfony/yaml": "^4.4",
        "ext-json": "*"
    },
    "autoload" : {
        "psr-4" : {
            "Simtt\\" : "src"
        }
    }
}
